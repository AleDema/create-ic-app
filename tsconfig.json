{
  "compilerOptions": {
    "target": "es2020",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ES2020"
    ],
    "types": [
      "vite/client"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "module": "es2020",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react",
    "baseUrl": "."
  },
  "include": [
    "src/frontend"
, "vite.config.ts"  ]
}
